<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- oracle驱动jar路径 -->
	<classPathEntry location="F://package//mysql-connector-java-5.1.44-bin.jar" />
	<context id="mysql">
		<!-- 生成的Java文件的编码 -->
		<property name="javaFileEncoding" value="UTF-8" />
		<!-- 格式化java代码 -->
		<property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter" />
		<!-- 格式化XML代码 -->
		<property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter" />

		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<!-- <plugin type="org.mybatis.generator.plugins.SerializablePlugin" /> -->

		<!-- 自定义的Pagination，也需要打成jar依赖进来 -->
		<plugin type="com.yejg.mybatis.generator.plugins.OverIsMergeablePlugin" />
		<plugin type="com.yejg.mybatis.generator.plugins.MysqlPaginationPlugin" />

        <!-- 自定义注释 -->
        <commentGenerator type="com.yejg.mybatis.generator.plugins.MyCommentGenerator" />
        

		<!-- jdbc连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" 
			connectionURL="jdbc:mysql://127.0.0.1:3306/test" userId="root" password="root">
		  <!-- 针对mysql数据库 -->
		  <property name="useInformationSchema" value="true" />
		</jdbcConnection>

		<!-- 生成的表对象路径 -->
		<javaModelGenerator targetPackage="com.yejg.model" targetProject="E:/generate" />
		<!-- 生成的xml存放路径 -->
		<sqlMapGenerator targetPackage="com.yejg.mapper.xml" targetProject="E:/generate" />
		<!-- 生成的dao mapper路径 -->
		<javaClientGenerator targetPackage="com.yejg.mapper" targetProject="E:/generate" type="XMLMAPPER" />

		<!-- 想要生成的表 -->
		<table schema="test" tableName="sys_user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
	</context>

</generatorConfiguration>  